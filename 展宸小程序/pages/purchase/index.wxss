.van-checkbox {
  width: 100%;
}

.van-swipe-cell__right {
  display: inline-block;
  width: 65px;
  height: 44px;
  font-size: 15px;
  line-height: 44px;
  color: #fff;
  text-align: center;
  background-color: #f44;
}

.cu-cell {
  margin-top: 20rpx;
}

.cu-cell .cu-icon {
  margin-top: 10rpx;
}

.mg-l {
  /* margin-left: 250rpx; */
  float: right;
  margin-right: 10rpx;
}

.mg-r {
  /* margin-left: 250rpx; */
  margin-left: 100rpx;
}

.remark {
  color: var(--color);
}

.loading {
  margin-top: 20rpx;
  margin-bottom: 200rpx;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}

.no-more {
  margin-top: 20rpx;
  margin-bottom: 200rpx;
  text-align: center;
  color: darkgray;
}

.addbut {
  position: fixed;
  bottom: 60rpx;
  width: 60%;
  left: 50%;
  transform: translate(-50%, 0%);
  -ms-transform: translate(-50%, 0%);
  -moz-transform: translate(-50%, 0%);
  -webkit-transform: translate(-50%, 0%);
  -o-transform: translate(-50%, 0%);
}

.delbox{
  margin-top: 150rpx;
}